<Window x:Class="TVTComment.Views.NiconicoOAuthWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:behaviors="clr-namespace:TVTComment.Views.Behaviors"
        xmlns:wv2="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
        prism:ViewModelLocator.AutoWireViewModel="True"
        mc:Ignorable="d"
        Title="NiconicoOAuthWindow" Height="915" Width="980">
    <Grid>
        <wv2:WebView2>
            <i:Interaction.Behaviors>
                <behaviors:WebView2Behavior WebView2RequestRecived="{Binding RequestRecived}"/>
            </i:Interaction.Behaviors>
        </wv2:WebView2>
    </Grid>
</Window>
